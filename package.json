{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@bshada/nseapi",
  "version": "0.1.0",
  "description": "An unofficial TypeScript API for NSE India (based on NseIndiaApiTs)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bshada/nseapi.git"
  },
  "bugs": {
    "url": "https://github.com/bshada/nseapi/issues"
  },
  "homepage": "https://github.com/bshada/nseapi#readme",
  "author": "bshada",
  "contributors": [],
  "type": "module",
  "browser": false,
  "main": "dist-cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json && node scripts/postbuild-cjs.js",
    "clean": "rimraf dist dist-cjs",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:network": "NSEAPI_NETWORK=1 vitest run",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "dist-cjs",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nse",
    "nseindia",
    "stocks",
    "api",
    "typescript",
    "nseapi"
  ],
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "axios-cookiejar-support": "^6.0.2",
    "got": "^12.6.1",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.6",
    "@types/node": "^22.7.4",
    "@types/tough-cookie": "^4.0.5",
    "eslint": "^9.11.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
